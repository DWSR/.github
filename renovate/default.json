{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "automerge": false,
  "platformAutomerge": false,
  "extends": [
    ":dependencyDashboard",
    ":timezone(America/Toronto)",
    ":rebaseStalePrs",
    ":prHourlyLimitNone",
    ":prImmediately"
  ],
  "enabledManagers": [
    "kustomize",
    "regex"
  ],
  "kustomize": {
    "ignorePaths": [
      "**/upstream/**",
      "apps/image-cache/base/spegel/kustomization.yaml",
      "apps/grafana/base/operator/kustomization.yaml"
    ]
  },
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "(^|/)vendir\.yml$",
        "^fly.ya?ml$"
      ],
      "matchStrings": [
        "# renovate: datasource=(?<datasource>.*?)(?:\\s+extractVersion=(?<extractVersion>.+?))?(?:\\s+versioning=(?<versioning>.+?))?\\s+depName=(?<depName>.+?)?\\s(?:\\s+.*:\\s+\"?(?<currentValue>[^\"\\n]*))\"?"
      ],
      "versioningTemplate": "{{#if versioning}}{{versioning}}{{else}}semver{{/if}}"
    }
  ],
  "hostRules": [
    {
      "matchHost": "docker.io",
      "username": "dwsr",
      "encrypted": {
        "password": ""
      }
    }
  ]
}
